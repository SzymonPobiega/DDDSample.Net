<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="DDDSample.Domain" namespace="DDDSample.Domain.Voyage">
  <class name="Voyage" table="Voyage">
    <id type="Guid">
      <column name="Id" />
      <generator class="guid"></generator>
    </id>
    <component name="Number" update="false" class="VoyageNumber">
      <property name="_number" column="Number" not-null="true" access="field"/>
    </component>    
  </class>
</hibernate-mapping>